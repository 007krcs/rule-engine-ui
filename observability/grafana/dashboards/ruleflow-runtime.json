{
  "uid": "ruleflow-runtime-v1",
  "title": "RuleFlow Runtime",
  "tags": ["ruleflow", "runtime", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "env",
        "type": "query",
        "label": "Environment",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "query": {
          "query": "label_values(api_call_count, env)",
          "refId": "EnvVariableQuery"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "current": {
          "selected": true,
          "text": "All",
          "value": ".*"
        }
      },
      {
        "name": "tenant_id",
        "type": "query",
        "label": "Tenant",
        "datasource": {
          "type": "prometheus",
          "uid": "PROMETHEUS_DS"
        },
        "query": {
          "query": "label_values(api_call_count{env=~\"$env\"}, tenant_id)",
          "refId": "TenantVariableQuery"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "current": {
          "selected": true,
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "API Calls / sec",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(api_call_count{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m]))",
          "legendFormat": "api calls/s"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "API Error Rate / sec",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(error_count{env=~\"$env\",tenant_id=~\"$tenant_id\",source=\"api\"}[5m]))",
          "legendFormat": "errors/s"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "API p95 Latency",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(api_latency_ms_bucket{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m])))",
          "legendFormat": "p95"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Rule Matches / sec",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rule_match_count{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m]))",
          "legendFormat": "rule matches/s"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "API Request Rate by Status Class",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status_class) (rate(api_call_count{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m]))",
          "legendFormat": "{{status_class}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Latency p50 / p95",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum by (le) (rate(api_latency_ms_bucket{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m])))",
          "legendFormat": "p50"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(api_latency_ms_bucket{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m])))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Rule Engine Throughput",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rule_eval_count{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m]))",
          "legendFormat": "rule eval/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate(rule_match_count{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m]))",
          "legendFormat": "rule match/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Flow Transitions",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(flow_transitions_count{env=~\"$env\",tenant_id=~\"$tenant_id\"}[5m]))",
          "legendFormat": "flow transitions/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    }
  ]
}
